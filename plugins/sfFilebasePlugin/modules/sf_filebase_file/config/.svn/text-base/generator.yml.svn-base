generator:
  class: sfDoctrineGenerator
  param:
    model_class:           sfFilebaseFile
    theme:                 admin
    non_verbose_templates: true
    with_show:             false
    singular:              ~
    plural:                ~
    route_prefix:          sf_filebase_file
    with_doctrine_route:     1

    config:
      actions: ~
      fields:
        pathname:
          label: File
        sf_filebase_directories_id:
          label: Directory
        new_name:
          label: Rename file
      list:    ~
      filter:  ~
      form:    ~
      edit:
        display: [filename, sf_filebase_directories_id, comment]
      new:
        display: [filename, sf_filebase_directories_id, comment]
